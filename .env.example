# Database Connection
# Get this from Supabase Dashboard → Settings → Database
# IMPORTANT: Use port 6543 (connection pooling) not 5432
DATABASE_URL="postgresql://postgres:YOUR_PASSWORD@db.YOUR_PROJECT.supabase.co:6543/postgres?sslmode=require"

# NextAuth Secret
# Generate with: openssl rand -base64 32
NEXTAUTH_SECRET="your_generated_secret_here"

# Application URLs
NEXTAUTH_URL="http://localhost:3000"
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Google OAuth Credentials
# Get these from Google Cloud Console → APIs & Services → Credentials
GOOGLE_CLIENT_ID="your_google_client_id"
GOOGLE_CLIENT_SECRET="your_google_client_secret"
